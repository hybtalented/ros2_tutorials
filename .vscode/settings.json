{
  "files.associations": {
    "*.template": "plaintext",
    "*.json": "json",
    "*.qs": "javascript",
    "*.qdoc": "cpp",
    "*.doc": "cpp",
    "*.in": "plaintext",
    "*.html": "html",
    "*.vuehtml": "vue-html",
    "*.conf": "nginx",
    "chrono": "cpp",
    "functional": "cpp",
    "memory": "cpp",
    "random": "cpp",
    "string": "cpp"
  },
  "python.autoComplete.extraPaths": [
    "/opt/ros/rolling/lib/python3.10/site-packages",
    "/opt/ros/rolling/local/lib/python3.10/dist-packages"
  ],
  "python.analysis.extraPaths": [
    "/opt/ros/rolling/lib/python3.10/site-packages",
    "/opt/ros/rolling/local/lib/python3.10/dist-packages"
  ],
  "terminal.integrated.profiles.linux": {
    "ros": {
      "id": "ros",
      "title": "ros终端",
      "path": "/bin/bash",
      "args": [
        "--init-file",
        "${workspaceFolder}/setup.sh"
      ]
    }
  },
  "terminal.integrated.defaultProfile.linux": "ros"
}